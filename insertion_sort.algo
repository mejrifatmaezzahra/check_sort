FUNCTION insertionSort(n:integer , VAR arr:ARRAY_OF INTEGER[n]): array
VAR
    i,key,j: INTEGER;
    FOR i FROM 1 to N
        key := arr[i];
        j := i - 1;
 
        WHILE (j >= 0 && arr[j] > key) DO
            arr[j + 1] := arr[j];
            j := j - 1;
        END_WHILE
        arr[j + 1] := key;
        write(arr[i]);
    END_FOR
    


ALGORITHM Insertion 
VAR
tab:ARRAY_OF INTEGER[n]
BEGIN 
tab:=insertionSort(arr, n);
END
 
